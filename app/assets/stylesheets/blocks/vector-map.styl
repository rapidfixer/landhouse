.b-vector-map
  position relative
  width 860px
  height 460px
#vector-map
  width 100%
  height 100%
  background-image url('/assets/img/vector-map-bg.png')
  background-position top left
  background-repeat no-repeat
  &__helper
    position absolute
    right 0
    top 0
    background black
    color white
    &.debug-mode
      color red
